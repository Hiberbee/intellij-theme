<idea-plugin>
  <change-notes>Fixed plugin page buttons colors. Updated theme colors for debugger, selections &amp; static/nonstatic
    methods
  </change-notes>
  <id>com.hiberbee.intellij.hiberbee-theme</id>
  <vendor email="vlad@hiberbee.com" url="https://github.com/hiberbee">hiberbee</vendor>
  <name>Hiberbee theme</name>
  <version>@version@</version>
  <description>Monokai Pro &amp; MacOS Mojave inspired color themes, with bright colors &amp; dark mode, with common
    color code for all languages. See description &amp; screenshots on &lt;a href="https://github.com/Hiberbee/code-highlight-themes"&gt;Github&lt;/a&gt;

    Check the sources, iTerm2, Windows and macOS Terminals and XCode color schemes also available
  </description>
  <idea-version since-build="191.6183"/>
  <depends>com.intellij.modules.lang</depends>
  <extensions defaultExtensionNs="com.intellij">
    <bundledColorScheme path="/colors/Dark" id="2adc3da4-e7de-43ff-992b-1505442df2c4"/>
    <themeProvider id="683fb0ab-974f-4604-8b3d-8ed74934e18d" path="/HiberbeeDark.theme.json"/>
  </extensions>
</idea-plugin>
